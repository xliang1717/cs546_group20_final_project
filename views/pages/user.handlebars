<div id="usercontainer" class="usercontainer" data-userId="{{userId}}">
  <div>Username:
    <span id="nickname">{{ userInfo.nickname }}</span>
  </div>

  <div>Firstname:
    <span id="firstName">{{ userInfo.firstName }}</span>
  </div>

  <div>Lastname:
    <span id="lastName">{{ userInfo.lastName }}</span>
  </div>

  <div>Email:
    <span id="email">{{ userInfo.email }}</span>
  </div>

  <div><a href="/user/myprofile">Edit Profile</a></div>

  <div class="titleDiv">My Parking Spot</div>
  <div id="carParkingLocation">{{ userInfo.carParkingLocation }}</div>
  <br>

  {{!-- <div>My Car</div>
  <div id="carlistDiv" class="carlistDiv">
